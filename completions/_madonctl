_madonctl_status()
{
    last_command="madonctl_status"

    command_aliases=()

    commands=()
    commands+=("boost")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("reblog")
        aliashash["reblog"]="boost"
    fi
    commands+=("card")
    commands+=("context")
    commands+=("delete")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("rm")
        aliashash["rm"]="delete"
    fi
    commands+=("favourite")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("fave")
        aliashash["fave"]="favourite"
        command_aliases+=("favorite")
        aliashash["favorite"]="favourite"
    fi
    commands+=("favourited-by")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("favorited-by")
        aliashash["favorited-by"]="favourited-by"
    fi
    commands+=("mute-conversation")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("mute")
        aliashash["mute"]="mute-conversation"
    fi
    commands+=("pin")
    commands+=("post")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("pouet")
        aliashash["pouet"]="post"
        command_aliases+=("toot")
        aliashash["toot"]="post"
    fi
    commands+=("reblogged-by")
    commands+=("show")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("display")
        aliashash["display"]="show"
    fi
    commands+=("unboost")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("unreblog")
        aliashash["unreblog"]="unboost"
    fi
    commands+=("unfavourite")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("unfave")
        aliashash["unfave"]="unfavourite"
        command_aliases+=("unfavorite")
        aliashash["unfavorite"]="unfavourite"
    fi
    commands+=("unmute-conversation")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("unmute")
        aliashash["unmute"]="unmute-conversation"
    fi
    commands+=("unpin")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--all")
    flags+=("--keep=")
    two_word_flags+=("--keep")
    two_word_flags+=("-k")
    flags+=("--limit=")
    two_word_flags+=("--limit")
    two_word_flags+=("-l")
    flags+=("--status-id=")
    two_word_flags+=("--status-id")
    two_word_flags+=("-s")
    flags+=("--color=")
    two_word_flags+=("--color")
    flags_with_completion+=("--color")
    flags_completion+=("__madonctl_color")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--instance=")
    two_word_flags+=("--instance")
    two_word_flags+=("-i")
    flags+=("--login=")
    two_word_flags+=("--login")
    two_word_flags+=("-L")
    flags+=("--output=")
    two_word_flags+=("--output")
    flags_with_completion+=("--output")
    flags_completion+=("__madonctl_output")
    two_word_flags+=("-o")
    flags_with_completion+=("-o")
    flags_completion+=("__madonctl_output")
    flags+=("--password=")
    two_word_flags+=("--password")
    two_word_flags+=("-P")
    flags+=("--template=")
    two_word_flags+=("--template")
    flags+=("--template-file=")
    two_word_flags+=("--template-file")
    flags+=("--theme=")
    two_word_flags+=("--theme")
    flags_with_completion+=("--theme")
    flags_completion+=("__madonctl_theme")
    flags+=("--token=")
    two_word_flags+=("--token")
    two_word_flags+=("-t")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

