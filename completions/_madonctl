_madonctl_account()
{
    last_command="madonctl_account"

    command_aliases=()

    commands=()
    commands+=("block")
    commands+=("blocks")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("blocked")
        aliashash["blocked"]="blocks"
    fi
    commands+=("favourites")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("favorited")
        aliashash["favorited"]="favourites"
        command_aliases+=("favorites")
        aliashash["favorites"]="favourites"
        command_aliases+=("favourited")
        aliashash["favourited"]="favourites"
    fi
    commands+=("follow")
    commands+=("follow-requests")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("follow-request")
        aliashash["follow-request"]="follow-requests"
        command_aliases+=("fr")
        aliashash["fr"]="follow-requests"
    fi
    commands+=("followers")
    commands+=("following")
    commands+=("mute")
    commands+=("mutes")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("muted")
        aliashash["muted"]="mutes"
    fi
    commands+=("notifications")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("notif")
        aliashash["notif"]="notifications"
        command_aliases+=("notification")
        aliashash["notification"]="notifications"
    fi
    commands+=("pin")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("endorse")
        aliashash["endorse"]="pin"
    fi
    commands+=("pinned")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("get-endorsements")
        aliashash["get-endorsements"]="pinned"
        command_aliases+=("list-endorsements")
        aliashash["list-endorsements"]="pinned"
    fi
    commands+=("relationships")
    commands+=("reports")
    commands+=("search")
    commands+=("show")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("display")
        aliashash["display"]="show"
    fi
    commands+=("statuses")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("st")
        aliashash["st"]="statuses"
    fi
    commands+=("unblock")
    commands+=("unfollow")
    commands+=("unmute")
    commands+=("unpin")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("disavow")
        aliashash["disavow"]="unpin"
    fi
    commands+=("update")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--account-id=")
    two_word_flags+=("--account-id")
    two_word_flags+=("-a")
    flags+=("--all")
    flags+=("--keep=")
    two_word_flags+=("--keep")
    two_word_flags+=("-k")
    flags+=("--limit=")
    two_word_flags+=("--limit")
    two_word_flags+=("-l")
    flags+=("--max-id=")
    two_word_flags+=("--max-id")
    flags+=("--since-id=")
    two_word_flags+=("--since-id")
    flags+=("--user-id=")
    two_word_flags+=("--user-id")
    two_word_flags+=("-u")
    flags+=("--color=")
    two_word_flags+=("--color")
    flags_with_completion+=("--color")
    flags_completion+=("__madonctl_color")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--instance=")
    two_word_flags+=("--instance")
    two_word_flags+=("-i")
    flags+=("--login=")
    two_word_flags+=("--login")
    two_word_flags+=("-L")
    flags+=("--output=")
    two_word_flags+=("--output")
    flags_with_completion+=("--output")
    flags_completion+=("__madonctl_output")
    two_word_flags+=("-o")
    flags_with_completion+=("-o")
    flags_completion+=("__madonctl_output")
    flags+=("--password=")
    two_word_flags+=("--password")
    two_word_flags+=("-P")
    flags+=("--template=")
    two_word_flags+=("--template")
    flags+=("--template-file=")
    two_word_flags+=("--template-file")
    flags+=("--theme=")
    two_word_flags+=("--theme")
    flags_with_completion+=("--theme")
    flags_completion+=("__madonctl_theme")
    flags+=("--token=")
    two_word_flags+=("--token")
    two_word_flags+=("-t")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

