_madonctl_toot()
{
    last_command="madonctl_toot"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--add-mentions")
    local_nonpersistent_flags+=("--add-mentions")
    flags+=("--file=")
    two_word_flags+=("--file")
    two_word_flags+=("-f")
    local_nonpersistent_flags+=("--file=")
    flags+=("--in-reply-to=")
    two_word_flags+=("--in-reply-to")
    two_word_flags+=("-r")
    local_nonpersistent_flags+=("--in-reply-to=")
    flags+=("--media-ids=")
    two_word_flags+=("--media-ids")
    local_nonpersistent_flags+=("--media-ids=")
    flags+=("--same-visibility")
    local_nonpersistent_flags+=("--same-visibility")
    flags+=("--sensitive")
    local_nonpersistent_flags+=("--sensitive")
    flags+=("--spoiler=")
    two_word_flags+=("--spoiler")
    local_nonpersistent_flags+=("--spoiler=")
    flags+=("--stdin")
    local_nonpersistent_flags+=("--stdin")
    flags+=("--text-file=")
    two_word_flags+=("--text-file")
    local_nonpersistent_flags+=("--text-file=")
    flags+=("--visibility=")
    two_word_flags+=("--visibility")
    flags_with_completion+=("--visibility")
    flags_completion+=("__madonctl_visibility")
    local_nonpersistent_flags+=("--visibility=")
    flags+=("--color=")
    two_word_flags+=("--color")
    flags_with_completion+=("--color")
    flags_completion+=("__madonctl_color")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--instance=")
    two_word_flags+=("--instance")
    two_word_flags+=("-i")
    flags+=("--login=")
    two_word_flags+=("--login")
    two_word_flags+=("-L")
    flags+=("--output=")
    two_word_flags+=("--output")
    flags_with_completion+=("--output")
    flags_completion+=("__madonctl_output")
    two_word_flags+=("-o")
    flags_with_completion+=("-o")
    flags_completion+=("__madonctl_output")
    flags+=("--password=")
    two_word_flags+=("--password")
    two_word_flags+=("-P")
    flags+=("--template=")
    two_word_flags+=("--template")
    flags+=("--template-file=")
    two_word_flags+=("--template-file")
    flags+=("--theme=")
    two_word_flags+=("--theme")
    flags_with_completion+=("--theme")
    flags_completion+=("__madonctl_theme")
    flags+=("--token=")
    two_word_flags+=("--token")
    two_word_flags+=("-t")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

