_madonctl_account_follow()
{
    last_command="madonctl_account_follow"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--remote=")
    two_word_flags+=("--remote")
    two_word_flags+=("-r")
    local_nonpersistent_flags+=("--remote=")
    flags+=("--show-reblogs")
    local_nonpersistent_flags+=("--show-reblogs")
    flags+=("--account-id=")
    two_word_flags+=("--account-id")
    two_word_flags+=("-a")
    flags+=("--all")
    flags+=("--color=")
    two_word_flags+=("--color")
    flags_with_completion+=("--color")
    flags_completion+=("__madonctl_color")
