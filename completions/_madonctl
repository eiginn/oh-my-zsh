_madonctl_lists()
{
    last_command="madonctl_lists"

    command_aliases=()

    commands=()
    commands+=("accounts")
    commands+=("add-accounts")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("add-account")
        aliashash["add-account"]="add-accounts"
    fi
    commands+=("create")
    commands+=("delete")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("del")
        aliashash["del"]="delete"
        command_aliases+=("rm")
        aliashash["rm"]="delete"
    fi
    commands+=("remove-accounts")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("remove-account")
        aliashash["remove-account"]="remove-accounts"
    fi
    commands+=("show")
    if [[ -z "${BASH_VERSION}" || "${BASH_VERSINFO[0]}" -gt 3 ]]; then
        command_aliases+=("display")
        aliashash["display"]="show"
        command_aliases+=("get")
        aliashash["get"]="show"
        command_aliases+=("ls")
        aliashash["ls"]="show"
    fi
    commands+=("update")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--all")
    flags+=("--keep=")
    two_word_flags+=("--keep")
    two_word_flags+=("-k")
    flags+=("--limit=")
    two_word_flags+=("--limit")
    two_word_flags+=("-l")
    flags+=("--list-id=")
    two_word_flags+=("--list-id")
    two_word_flags+=("-G")
    flags+=("--color=")
    two_word_flags+=("--color")
    flags_with_completion+=("--color")
    flags_completion+=("__madonctl_color")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--instance=")
    two_word_flags+=("--instance")
    two_word_flags+=("-i")
    flags+=("--login=")
    two_word_flags+=("--login")
    two_word_flags+=("-L")
    flags+=("--output=")
    two_word_flags+=("--output")
    flags_with_completion+=("--output")
    flags_completion+=("__madonctl_output")
    two_word_flags+=("-o")
    flags_with_completion+=("-o")
    flags_completion+=("__madonctl_output")
    flags+=("--password=")
    two_word_flags+=("--password")
    two_word_flags+=("-P")
    flags+=("--template=")
    two_word_flags+=("--template")
    flags+=("--template-file=")
    two_word_flags+=("--template-file")
    flags+=("--theme=")
    two_word_flags+=("--theme")
    flags_with_completion+=("--theme")
    flags_completion+=("__madonctl_theme")
    flags+=("--token=")
    two_word_flags+=("--token")
    two_word_flags+=("-t")
    flags+=("--verbose")
    flags+=("-v")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

